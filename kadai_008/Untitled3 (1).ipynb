        "import random\n",
        "\n",
        "var = random.randint(0,15)\n",
        "\n",
        "print(var)\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "   print(\"FizzzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "   print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "   print(\"Buzz\")\n",
        "else:\n",
        "    print(var)"
      ]
